                    var doc = new jsPDF('l', 'mm', 'a4');
                    var width = doc.internal.pageSize.width;
                    var height = doc.internal.pageSize.height;
                    doc.addImage(img, 'JPEG', 0, 0, width, height, 'center');
                    doc.save('test.pdf');

